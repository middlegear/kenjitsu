{
  "include": ["src"],
  "exclude": ["**/*.test.ts","node_modules"],
  "compilerOptions": {
    "target": "ESNext" ,
    "module": "ESNext" ,
    "moduleResolution": "node10", 
    "noEmit": false,
    "emitDeclarationOnly": false,
    "isolatedModules": false,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true, 
    "lib": ["ESNext"],
    
  }
}